put lib/h2o/service.py /lib/h2o/service.py
put lib/h2o/sensor.py /lib/h2o/sensor.py
put lib/h2o/actuator.py /lib/h2o/actuator.py

put h2o_boot.py boot.py
put h2o_main.py main.py
put h2o_config.txt config.txt
